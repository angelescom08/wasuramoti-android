  <string name="fudasetedit_fudanum_html">
    <![CDATA[
    <h3>Select By Poem Number</h3>
    <div><b>?</b> &rArr; any single character</div>
    <div><b>*</b> &rArr; any string, including empty string</div>
    <div><b>[xyz]</b> &rArr; x, y or z</div>
    <h3>Examples</h3>
    <div><b>8?</b> &rArr; 80,81, ..., 89</div>
    <div><b>*[13579]</b> &rArr; odd number</div>
    ]]>
  </string>
